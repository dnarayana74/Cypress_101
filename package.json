{
  "name": "cypress_101",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "cypress run",
    "test:headed": "cypress open",
     "merge-reports": "npx mochawesome-merge cypress/reports/mochawesome/*.json > cypress/reports/mochawesome/output.json",
    "generate-report": "npx marge cypress/reports/mochawesome/output.json --reportDir cypress/reports/mochawesome --inline",
    "posttest": "npm run merge-reports && npm run generate-report"
  },
  "devDependencies": {
    "@cypress-audit/lighthouse": "^1.4.2",
    "@cypress-audit/pa11y": "^1.0.1",
    "axe-core": "^4.10.3",
    "cypress": "^15.4.0",
    "cypress-audit": "^1.1.0",
    "cypress-axe": "^1.4.0",
    "cypress-xpath": "^2.0.0",
    "lighthouse": "^12.8.2",
    "mocha-junit-reporter": "^2.2.1",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.3.0"
  }
}
